{
  "name": "eos-block-explorer",
  "version": "1.0.0",
  "author": "Kylan Hurt <kylan.hurt@gmail.com> (https://github.com/kylanhurt)",
  "private": true,
  "dependencies": {
    "@types/node-fetch": "^2.5.2",
    "@types/react-redux": "^7.1.4",
    "bfj": "^6.1.1",
    "bootstrap": "^4.3.1",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "chalk": "^2.4.2",
    "classnames": "^2.2.6",
    "dotenv": "^6.2.0",
    "dotenv-expand": "^4.2.0",
    "eosjs": "^20.0.0",
    "file-loader": "^3.0.1",
    "fs-extra": "^7.0.1",
    "html-react-parser": "^0.9.1",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-pnp-resolver": "^1.2.1",
    "jest-resolve": "^24.3.1",
    "mdi-react": "^6.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-fetch": "^2.6.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "pnp-webpack-plugin": "^1.4.1",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "postcss-safe-parser": "^4.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-app-polyfill": "^0.2.2",
    "react-dev-utils": "6.0.3",
    "react-dom": "^16.8.4",
    "react-redux": "^7.1.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-smooth-scrollbar": "^8.0.6",
    "reactstrap": "^7.1.0",
    "redux": "^4.0.1",
    "redux-form": "^8.1.0",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "resolve": "1.10.0",
    "ricardian-template-toolkit": "^0.2.0",
    "smooth-scrollbar": "^8.3.1",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "1.2.3",
    "url-loader": "1.1.2"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js && yarn build-test",
    "test": "mocha \"src/tests/*.js\" --timeout 10000",
    "run-jest": "jest",
    "flow": "flow",
    "flow-coverage": "flow-coverage-report",
    "flow:0": "flow; exit 0",
    "prep": "rimraf prep && sucrase ./src/ -d ./prep --transforms imports,flow,jsx",
    "build-test": "rimraf build/tests && sucrase ./src/tests -d ./build/tests --transforms imports",
    "precommit": "yarn flow && yarn test"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@svgr/webpack": "4.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "^24.9.0",
    "babel-loader": "8.0.5",
    "babel-plugin-named-asset-import": "^0.3.1",
    "babel-preset-react-app": "^7.0.2",
    "babel-runtime": "6.26.0",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "css-loader": "2.1.1",
    "eslint": "5.15.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-loader": "2.1.2",
    "eslint-plugin-flowtype": "3.4.2",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.12.4",
    "flow-bin": "^0.108.0",
    "mocha": "^5.2.0",
    "node-sass": "^4.11.0",
    "react-hot-loader": "^4.8.0",
    "react-test-renderer": "^16.10.2",
    "sass-loader": "7.1.0",
    "sucrase": "^3.10.1",
    "webpack": "4.29.6",
    "webpack-dev-server": "3.2.1",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "4.1.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
